# Since the ".env" file is gitignored, you can use the ".env.example" file to
# build a new ".env" file when you clone the repo. Keep this file up-to-date
# when you add new variables to `.env`.

# This file will be committed to version control, so make sure not to have any
# secrets in it. If you are cloning this repo, create a copy of this file named
# ".env" and populate it with your secrets.

# When adding additional environment variables, the schema in "/src/env.js"
# should be updated accordingly.

# Drizzle
DATABASE_URL="postgresql://postgres:password@localhost:5432/pathway"

# AI - OpenRouter
OPENROUTER_API_KEY=""
OPENROUTER_MODEL="moonshotai/kimi-k2.5"
OPENROUTER_FALLBACK_MODEL="minimax/minimax-m2.5"
OPENROUTER_FAST_MODEL=""
INTELLIGENCE_AGENT_MODE="off"
INTELLIGENCE_TURN_BUDGET_MS="4500"
INTELLIGENCE_TURN_BUDGET_FIRST_MS="9000"
INTELLIGENCE_TURN_BUDGET_FOLLOWUP_MS="4500"
INTELLIGENCE_TURN_REPAIR_RESERVE_MS="1200"
INTELLIGENCE_AGENT_MAX_TOKENS_FIRST_TURN="700"
INTELLIGENCE_AGENT_MAX_TOKENS_FOLLOWUP="900"
INTELLIGENCE_AGENT_MAX_TOKENS_REPAIR="600"

# Data providers
OPENWEATHER_API_KEY=""
TICKETMASTER_API_KEY=""

# Maps (choose one or both)
NEXT_PUBLIC_MAPBOX_ACCESS_TOKEN=""
NEXT_PUBLIC_GOOGLE_MAPS_API_KEY=""
GOOGLE_PLACES_API_KEY=""
NYC_DOT_CLOSURES_URL=""
NYC_OPEN_DATA_APP_TOKEN=""

# Observability
NEXT_PUBLIC_POSTHOG_KEY=""
NEXT_PUBLIC_POSTHOG_HOST="https://app.posthog.com"
